<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:fa ="http://schemas.awesome.incremented/wpf/xaml/fontawesome.sharp">

    <!-- Colour Pallates -->
    <!--*******************************************************************************************************************************************************************************-->


    <!-- Semi Dark-->
    <Color x:Key="Primary" >#232f3e</Color>
    <SolidColorBrush x:Key="PrimaryBrush">#232f3e</SolidColorBrush>

    <!-- Light -->
    <Color x:Key="Secondary">#ff9900</Color>
    <SolidColorBrush x:Key="SecondaryBrush">#ff9900</SolidColorBrush>

    <!-- Darkest -->
    <Color x:Key="Menu">#182029</Color>
    <SolidColorBrush x:Key="MenuBrush">#182029</SolidColorBrush>

    <!-- Semi Light -->
    <Color x:Key="Tertiary">#146eb4</Color>
    <SolidColorBrush x:Key="TertiaryBrush">#146eb4</SolidColorBrush>

    <!-- Foreground -->
    <Color x:Key="Foreground">#f2f2f2</Color>
    <SolidColorBrush x:Key="ForegroundBrush">#f2f2f2</SolidColorBrush>
    <!--*******************************************************************************************************************************************************************************-->

</ResourceDictionary>