<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:fa ="http://schemas.awesome.incremented/wpf/xaml/fontawesome.sharp">


    <!-- Dark -->
    <Color x:Key="Primary" >#c6f7ee</Color>
    <SolidColorBrush x:Key="PrimaryBrush">#c6f7ee</SolidColorBrush>

    <!-- Light-->

    <Color x:Key="Secondary">#09e1a8</Color>
    <SolidColorBrush x:Key="SecondaryBrush">#09e1a8</SolidColorBrush>

    <!-- Lightest -->

    <Color x:Key="Menu">#e9eceb</Color>
    <SolidColorBrush x:Key="MenuBrush">#e9eceb</SolidColorBrush>

    <!-- Light -->

    <Color x:Key="Tertiary">#9bb9e8</Color>
    <SolidColorBrush x:Key="TertiaryBrush">#9bb9e8</SolidColorBrush>

    <!-- Foreground -->

    <Color x:Key="Foreground">#000000</Color>
    <SolidColorBrush x:Key="ForegroundBrush">#000000</SolidColorBrush>

    <!--*******************************************************************************************************************************************************************************-->


  
</ResourceDictionary>